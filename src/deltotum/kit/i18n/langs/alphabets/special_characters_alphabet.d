/**
 * Authors: initkfs
 */
class SpecialCharactersAlphabet : Alphabet
{
    override immutable(dchar)[] allLetters() pure
    {
        dchar[] letters = "ð‘‘›!Ã—\"#$%&\'()*+,-./:;<=>?@[\\]^_`{|}~ \n\r\t\b"d.dup;
        //Box-drawing characters
        foreach (dchar ch; '\u2500' .. '\u257F')
        {
            letters ~= ch;
        }
        return letters;
    }
}
