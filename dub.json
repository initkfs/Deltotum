{
    "description": "A simple graphics experiment.",
    "license": "proprietary",
    "authors": [
        "initkfs"
    ],
    "copyright": "Copyright Â© 2022, initkfs",
    "name": "deltotum",
    "configurations": [{
            "name": "app-dev",
            "versions": [
                "dev", "SdlBackend", "ExtensionLua", "BuiltinProfiler"
            ],
            "targetType": "executable",
            "targetName": "deltotum",
            "platforms": [
                "linux"
            ]
        },
        {
            "name": "app-dist",
            "versions": [
                "dev", "SdlBackend", "BuiltinProfiler"
            ],
            "targetType": "executable",
            "targetName": "./dist/deltotum",
            "platforms": [
                "linux"
            ]
        },
        {
            "name": "app-trace",
            "versions": [
                "dev", "SdlBackend"
            ],
            "targetType": "executable",
            "targetName": "deltotum",
            "platforms": [
                "linux"
            ],
            "dflags-ldc": [
                "--ftime-trace", "--ftime-trace-file=$PACKAGE_DIR/trace.json"
            ],
            "postBuildCommands": ["timetrace2txt $PACKAGE_DIR/trace.json --tsv $PACKAGE_DIR/trace.tsv -o $PACKAGE_DIR/trace.txt"]
        }
    ],
    "buildRequirements": ["disallowDeprecations", "allowWarnings"],
    "sourcePaths": ["./src"],
    "importPaths": ["./src"],
    "dependencies": {
        "bindbc-sdl": "~>1.2.1"
    },
    "dflags": ["-dip1000", "-preview=in", "-J=./data"],
    "versions": [
        "SDL_Image",
        "SDL_TTF",
        "SDL_2020",
        "SDL_Mixer_202",
        "Cairo116"
    ]
}
